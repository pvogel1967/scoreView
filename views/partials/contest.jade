.container-fluid
	.page-header
		.row
			.col-lg-1.col-md-2.col-sm-2.col-xs-12
				h3
					.label.label-primary Location:
			.col-md-4.col-lg-4.col-sm-10.col-xs-12
				h3 {{contestData.contestName}} -- {{contestData.location}}
			.col-lg-2.col-md-3.col-sm-4.col-xs-12
				h3
					.label.label-primary Contest Director:
			.col-lg-4.col-md-3.col-sm-8.col-xs-12
				h3 {{contestData.contestDirector}}
		.row
			.col-lg-1.col-md-2.col-sm-2.col-xs-12
				h3
					.label.label-primary District:
			.col-md-4.col-lg-4.col-sm-10.col-xs-12
				h3 {{contestData.district}}
			.col-lg-2.col-md-3.col-sm-4.col-xs-12
				h3
					.label.label-primary Date:
			.col-lg-4.col-md-3.col-sm-8.col-xs-12
				h3 {{contestData.date}}
	.row(ng-repeat="row in classRows")
		.div(ng-repeat="column in row",ng-class="columnClass")
			.panel.panel-primary
				.panel-heading
					h3.panel-title {{column.name}}
				.table-responsive
					table.table.table-striped.table-condensed.table-hover
						thead
							tr
								th Name
								th Final Score
								th(ng-repeat="round in column.contestants | roundfilter") Round {{round}}
						tbody
							tr(ng-repeat="contestant in column.contestants")
								td 
									a(ng-href="/{{contestData.contestID}}/class/{{column.code}}/contestant/{{contestant.realAmaNumber}}") {{contestant.name}}
								td {{contestant.finalScore}}
								td(ng-repeat="score in contestant.scoringData", ng-style="droppedStyle(score)") {{score.normalizedScore}}


